{
  "name": "@dev/nestjs-standalone-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e env.test --  npm run test:ci && npm run test:e2e",
    "test:unit": "jest --config ./jest.config.js --passWithNoTests",
    "test:migration": "dotenv -e env.test --  typeorm migration:run -t each --dataSource ./build/ormconfig.js",
    "test:ci": "NODE_ENV=CI jest --ci none --testPathPattern='src' --forceExit true --collectCoverage --maxWorkers 2 --detectOpenHandles --passWithNoTests",
    "pretest": "npm run test:migration",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e env.test -- jest --config jest.e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d ./build/ormconfig.js",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:show": "npm run typeorm migration:show",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:create": "typeorm-ts-node-commonjs migration:create"
  },
  "dependencies": {
    "@golevelup/ts-jest": "^0.3.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/aws-lambda": "^8.10.145",
    "@willsoto/nestjs-prometheus": "^6.0.1",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^7.4.2",
    "express-basic-auth": "^1.2.1",
    "file-type-mime": "^0.4.3",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mysql2": "^3.2.0",
    "pg": "^8.12.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.3.20",
    "url-join-ts": "^1.0.5",
    "uuid": "^8.3.2",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.12",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "_moduleAliases": {
    "@domain": "build/src/app/domain",
    "@core": "build/src/app/core",
    "@auth": "build/src/app/auth",
    "@lib": "build/src/lib",
    "@db": "build/src/db",
    "@config": "build/src/config",
    "@logger": "build/src/logger",
    "@swagger": "build/src/swagger"
  }
}
